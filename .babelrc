{
  "presets": [["@babel/preset-env", {
    "useBuiltIns": "usage",
    "modules": "auto",
    "corejs": 3
  }],
  "@babel/preset-react",
  "@babel/preset-typescript"],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "plugins": ["@babel/plugin-transform-modules-commonjs"]
    },
    "production": {
      "plugins": [
        ["transform-imports", {
          "@material-ui/core": {
            "transform": "@material-ui/core/esm/${member}",
            "preventFullImport": true
          },
          "@material-ui/icons": {
            "transform": "@material-ui/icons/esm/${member}",
            "preventFullImport": true
          }
        }]
      ]
    },
    "staging": {
      "plugins": [
        ["transform-imports", {
          "@material-ui/core": {
            "transform": "@material-ui/core/esm/${member}",
            "preventFullImport": true
          },
          "@material-ui/icons": {
            "transform": "@material-ui/icons/esm/${member}",
            "preventFullImport": true
          }
        }]
      ]
    },
    "development": {
      "plugins": [
        ["transform-imports", {
          "@material-ui/core": {
            "transform": "@material-ui/core/esm/${member}",
            "preventFullImport": true
          },
          "@material-ui/icons": {
            "transform": "@material-ui/icons/esm/${member}",
            "preventFullImport": true
          }
        }]
      ]
    },
    "local": {
      "plugins": [
        ["transform-imports", {
          "@material-ui/core": {
            "transform": "@material-ui/core/esm/${member}",
            "preventFullImport": true
          },
          "@material-ui/icons": {
            "transform": "@material-ui/icons/esm/${member}",
            "preventFullImport": true
          }
        }]
      ]
    }
  }
}
